{
  "name": "library-chatbot",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/libs/library-chatbot/src",
  "prefix": "chat-bot",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "apps/libs/library-chatbot/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "apps/libs/library-chatbot/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "apps/libs/library-chatbot/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/libs/library-chatbot/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/libs/library-chatbot/**/*.ts",
          "apps/libs/library-chatbot/**/*.html",
          "apps/libs/library-chatbot/package.json"
        ]
      }
    }
  }
}
